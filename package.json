{
  "name": "simagarcli",
  "version": "1.0.3",
  "description": "Elegant CLI Builder",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "simagar": "node ./dist/index.mjs"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "play": "jiti ./cli.ts"
  },
  "dependencies": {
    "c12": "^1.11.2",
    "consola": "^3.2.3",
    "giget": "^1.2.3",
    "pathe": "^1.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "^1.5.2",
    "automd": "^0.3.7",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "eslint-config-unjs": "^0.2.1",
    "jiti": "^1.21.0",
    "prettier": "^3.2.5",
    "scule": "^1.3.0",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vitest": "^1.5.2"
  },
  "packageManager": "pnpm@9.0.6"
}
